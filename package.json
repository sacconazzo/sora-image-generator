{
  "name": "sora-generate-puppeteer",
  "type": "module",
  "version": "1.0.1",
  "license": "MIT",
  "description": "A simple script to get Sora's contents",
  "scripts": {
    "start": "node app.js",
    "chromium": "chromium-browser --remote-debugging-port=9222 --no-first-run --no-default-browser-check --disable-features=TranslateUI --no-sandbox --disable-dev-shm-usage --disable-gpu"
  },
  "dependencies": {
    "puppeteer-core": "^24.7.2"
  }
}
